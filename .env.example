# TechLake Environment Configuration
# Copy this file to .env and fill in your values
# NEVER commit the .env file to version control

# Admin password hash (bcrypt) - generate with: php -r "echo password_hash('YOUR_PASSWORD', PASSWORD_BCRYPT, ['cost' => 12]);"
ADMIN_PASSWORD_HASH=

# GitHub webhook secret - set this in GitHub webhook settings too
GITHUB_WEBHOOK_SECRET=

# Deploy path on cPanel
DEPLOY_PATH=/home/YOUR_CPANEL_USERNAME/public_html

# Application environment (production or development)
APP_ENV=production
